{
  "name": "translate-json-object",
  "version": "0.0.0-development",
  "description": "Translate a JSON object to any language via google or yandex translate API",
  "main": "lib/translate-json-object.js",
  "scripts": {
    "commit": "git-cz",
    "commitmsg": "validate-commit-msg",
    "lint": "xo",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watchAll",
    "semantic-release": "semantic-release -d && npm publish && semantic-release",
    "docs": "jsdoc2md lib/translate-json-object.js > docs/api.md"
  },
  "release": {
    "branch": "master"
  },
  "dependencies": {
    "google-translate": "2.2.0",
    "lodash": "4.17.11",
    "promise": "7.1.1",
    "yandex-translate": "2.1.2"
  },
  "devDependencies": {
    "commitizen": "3.0.7",
    "cz-conventional-changelog": "1.2.0",
    "husky": "0.12.0",
    "jest": "24.4.0",
    "jsdoc-to-markdown": "4.0.1",
    "semantic-release": "15.13.4",
    "validate-commit-msg": "2.8.2",
    "xo": "0.17.1"
  },
  "xo": {
    "global": [
      "jest",
      "expect",
      "test",
      "describe",
      "it"
    ]
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 85,
        "functions": 85,
        "lines": 85,
        "statements": 85
      }
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "keywords": [
    "translate",
    "object",
    "JSON",
    "google",
    "yandex",
    "translate"
  ],
  "author": "Khaled Mohamed",
  "license": "MIT",
  "homepage": "https://github.com/KhaledMohamedP/translate-json-object#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/KhaledMohamedP/translate-json-object.git"
  },
  "bugs": {
    "url": "https://github.com/KhaledMohamedP/translate-json-object/issues"
  }
}
